<?xml version="1.0"?>
<package >
  <metadata>
    <id>Yoeca.Sql</id>
    <version>0.0.14</version>
    <title>Yoeca.Sql - simple SQL formatter</title>
    <authors>Bas van der Laken</authors>    
    <owners>bas@yoeca.nl</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Class library that allows formatting SQL strings through generics. Currently supports the MySql dialect.</description>
    <releaseNotes>Initial release.
    Support for:
    * creating &amp; dropping tables
    * inserting record
    * select records with simple where clause
    * binary blobs using proto buffers
    * integers, doubles, enums &amp; date-time
    * has table functionality
    * abstracted MySql implementation
    * added MySql UPSERT functionality
    </releaseNotes>
    <copyright>Copyright 2016</copyright>
    <tags>SQL</tags>
    <dependencies>
      <dependency id="protobuf-net" version="3.2.30" />
      <dependency id="MySql.Data" version="8.3." />
    </dependencies>
  </metadata>
  <files>
    <file src="bin\Release\Yoeca.Sql.dll" target="lib\net45" />
  </files>  
</package>
